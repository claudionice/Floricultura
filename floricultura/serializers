from rest_framework import serializers 
from floricultura.models import Planta, Vaso

class PlantaSerializers (serializers.ModelSerializers):
    class Meta ():
        model= Planta
        fields = '__all__'

class VasoSerializers (serializers.ModelSerializers):
    class Meta ():
        model= Vaso 
        fields = '__all__'